 */
package week2;

/**
 * @author Ron
 *
 */
public class Token {
	int[] TN;
	char[] TS;
	
	/**
	 * 
	 * @param size Number of Processes in the system
	 */
	public Token(int size){
		this.TN = new int[size];
		this.TS = new char[size];
		
		for (int i = 0; i < TN.length; i++) {
			this.TS[i] = 'O';
		}
	}
}
